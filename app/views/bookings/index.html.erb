
<section>
  <!--for demo wrap-->
  <h1>My Bookings</h1>
  <div class="tbl-header">
    <table cellpadding="0" cellspacing="0" >
      <thead>
        <tr>
          <th>Offer</th>
          <th>Status</th>
          <th>Start Date</th>
          <th>End date</th>
          <th>Price</th>
        </tr>
      </thead>
    </table>
  </div>
    <div class="tbl-content">
      <table cellpadding="0" cellspacing="0">
        <tbody>
            <% @bookings.each do |booking| %>
              <tr>
                <td> <%=link_to booking.offer.title, booking_path(booking)%> </td>
                <td> <%=booking.status%> </td>
                <td> <%=booking.start_date%> </td>
                <td> <%=booking.end_date%> </td>
                <td> <%=booking.total_price%> </td>
              </tr>
            <%end%>
        </tbody>
      </table>
    </div>
</section>


