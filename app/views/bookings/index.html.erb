
<section class="bg-gradient-green">
  <!--for demo wrap-->
  <h1>My Bookings</h1>
    <table id="bookings-table" cellpadding="0" cellspacing="0" >
      <thead>
        <tr>
          <th>Offer</th>
          <th>Status</th>
          <th>Start Date</th>
          <th>End date</th>
          <th>Price</th>
        </tr>
      </thead>
      <tbody>
          <% @bookings.each do |booking| %>
            <tr>
              <td> <%=booking.offer.title%> </td>
              <td> <%=booking.status%> </td>
              <td> <%=booking.start_date%> </td>
              <td> <%=booking.end_date%> </td>
              <td> <%=@total_price%> </td>
            </tr>
          <%end%>
      </tbody>
    </table>
</section>
